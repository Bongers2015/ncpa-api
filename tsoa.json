{
	"swagger": {
		"outputDirectory": "./src/swagger",
		"entryFile": "./src/server.ts",
		"basePath": "/api/tnm/v1",
		"noImplicitAdditionalProperties": "silently-remove-extras",
		"securityDefinitions": {
			"api_key": {
				"type": "apiKey",
				"name": "access_token",
				"in": "query"
			},
			"jwtAuth": {
				"type": "apiKey",
				"in": "header",
				"name": "authorization"
			}
		},
		"specMerging": "recursive",
		"spec": {
			"paths": {
				"/config/grid-currents": {
					"post": {
						"parameters": [
							{
								"in": "query",
								"name": "gridCurrents",
								"required": true,
								"type": "array",
								"items": {
									"type": "number"
								},
								"minItems": 1,
								"maxItems": 3
							},
							{
								"in": "query",
								"name": "clientId",
								"required": true,
								"type": "string"
							}
						]
					}
				},
				"/upgrade": {
					"post": {
						"consumes": [ "multipart/form-data" ],
						"parameters": [
							{
								"in": "formData",
								"name": "data",
								"required": true,
								"type": "file"
							},
							{
								"in": "query",
								"name": "clientId",
								"required": true,
								"type": "string"
							}
						]
					}
				}
			}
		}
	},
	"routes": {
		"basePath": "/api/tnm/v1",
		"entryFile": "./src/server.ts",
		"routesDir": "./src",
		"authenticationModule": "./src/authentication.ts"
	},
	"ignore": [ "**/node_modules/**" ]
}
