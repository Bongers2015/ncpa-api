{
	"basePath": "/api/tnm/freeapp/v1",
	"consumes": [
		"application/json"
	],
	"definitions": {
		"Card": {
			"properties": {
				"token": {
					"type": "string",
					"description": "RFID"
				},
				"status": {
					"type": "string",
					"enum": [
						"ACCEPTED",
						"BLOCKED",
						"EXPIRED",
						"INVALID"
					]
				},
				"expirationDate": {
					"type": "string",
					"x-nullable": true
				}
			},
			"required": [
				"token",
				"status"
			],
			"type": "object",
			"additionalProperties": false
		},
		"CardRegistration": {
			"properties": {
				"card": {
					"$ref": "#/definitions/Card",
					"x-nullable": true
				},
				"status": {
					"type": "string",
					"enum": [
						"SUCCESS",
						"FAILURE",
						"TIMEOUT"
					]
				},
				"statusMessage": {
					"type": "string"
				}
			},
			"required": [
				"status",
				"statusMessage"
			],
			"type": "object",
			"additionalProperties": false
		},
		"ChargePointStatus": {
			"properties": {
				"code": {
					"type": "string",
					"enum": [
						"0",
						"1",
						"2",
						"3",
						"4",
						"5",
						"6"
					]
				},
				"statusMessage": {
					"type": "string",
					"enum": [
						"Available",
						"Preparing",
						"Charging",
						"SuspendedEV",
						"SuspendedEVSE",
						"Finishing",
						"Faulted"
					]
				},
				"plugAndChargeEnabled": {
					"type": "boolean"
				},
				"numberOfRFIDCardsRegistered": {
					"type": "number",
					"format": "double"
				}
			},
			"required": [
				"code",
				"statusMessage",
				"plugAndChargeEnabled",
				"numberOfRFIDCardsRegistered"
			],
			"type": "object",
			"additionalProperties": false
		},
		"Transaction": {
			"properties": {
				"id": {
					"type": "string"
				},
				"token": {
					"type": "string",
					"description": "card token id"
				},
				"startDate": {
					"type": "string"
				},
				"stopDate": {
					"type": "string",
					"x-nullable": true
				},
				"stopReason": {
					"type": "string",
					"x-nullable": true
				},
				"startKWattHour": {
					"type": "number",
					"format": "double"
				},
				"stopKWattHour": {
					"type": "number",
					"format": "double",
					"x-nullable": true
				}
			},
			"required": [
				"id",
				"token",
				"startDate",
				"startKWattHour"
			],
			"type": "object",
			"additionalProperties": false
		},
		"Upgrade": {
			"properties": {
				"originalname": {
					"type": "string",
					"description": "Name of the file on the user's computer"
				},
				"encoding": {
					"type": "string",
					"description": "Encoding type of the file"
				},
				"mimetype": {
					"type": "string",
					"description": "Mime type of the file"
				},
				"size": {
					"type": "number",
					"format": "double",
					"description": "Size of the file in bytes"
				},
				"destination": {
					"type": "string",
					"description": "The folder to which the file has been saved (DiskStorage)"
				},
				"location": {
					"type": "string",
					"description": "The url where to get the uploaded file (aws S3 for example)"
				},
				"filename": {
					"type": "string",
					"description": "The name of the file within the destination (DiskStorage)"
				},
				"path": {
					"type": "string",
					"description": "Location of the uploaded file (DiskStorage)"
				}
			},
			"required": [
				"originalname",
				"encoding",
				"mimetype",
				"size",
				"destination",
				"location",
				"filename",
				"path"
			],
			"type": "object",
			"additionalProperties": false
		},
		"UpgradeResponse": {
			"properties": {
				"filename": {
					"type": "string"
				},
				"data": {
					"$ref": "#/definitions/Upgrade"
				}
			},
			"required": [
				"filename",
				"data"
			],
			"type": "object",
			"additionalProperties": false
		}
	},
